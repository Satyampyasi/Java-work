package Array_list;
import java.util.ArrayList;
public class Array {
public static void main(String[] args) {
	int arr[]  = new int[10];
	ArrayList<Integer>list = new ArrayList<>();
	list.add(10);
	list.add(20);
	System.out.println(list.get(1));
	list.add(1,50);
   for(int x:list) {
	   System.out.println(x);
   }
}
}
